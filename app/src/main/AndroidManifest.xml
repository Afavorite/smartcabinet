<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.app2018212763.smartcabinet">
    <!-- 开启INTERNET权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Smartcabinet"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".order.OrderDelActivity"
            android:exported="false" />
        <activity
            android:name=".order.OrderShowActivity"
            android:exported="false" />
        <activity
            android:name=".order.OrderAddActivity"
            android:exported="false" />
        <activity
            android:name=".http.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".http.RegisterActivity"
            android:exported="false" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>